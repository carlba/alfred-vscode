{
  "name": "alfred-vscode",
  "version": "3.0.0",
  "description": "Alfred workflow that allows you to browse and open Visual Studio Code (Insiders) projects",
  "type": "module",
  "license": "MIT",
  "repository": "carlba/alfred-vscode",
  "homepage": "https://github.com/carlba/alfred-vscode#readme",
  "author": {
    "name": "Carl Bäckström",
    "email": "genzorg@gmail.com",
    "url": "https://github.com/carlba"
  },
  "keywords": [
    "alfred",
    "workflow",
    "alfred-workflow",
    "alfy",
    "vscode",
    "visual-studio-code",
    "project",
    "manager",
    "project-manager"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5.2.0"
  },
  "files": [
    "index.js",
    "lib",
    "*.png",
    "info.plist",
    ".bin"
  ],
  "scripts": {
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup",
    "test": "npm run lint",
    "cm": "npx git-cz",
    "release": "npx standard-version",
    "start": "node index.js",
    "start:dev": "nodemon -r dotenv/config -q index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "alfy": "^2.0.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "fuzzysort": "^3.0.1",
    "prettier": "^3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "debug": "^4.3.5",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
